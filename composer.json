{
    "name": "afsdarif/translation-manager",
    "description": "Manage your application's translation strings in Filament.",
    "keywords": [
        "Kenepa",
        "laravel",
        "filament",
        "localization",
        "translation",
        "afsdarif"
    ],
    "homepage": "https://github.com/kenape/translation-manager",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Afsdarif\\TranslationManager\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "filament/notifications": "^4.0.0",
        "spatie/laravel-package-tools": "^1.92.4",
        "spatie/laravel-translation-loader": "^2.8.2",
        "stijnvanouplines/blade-country-flags": "^1.0.6"
    },
    "require-dev": {
        "tightenco/duster": "^2.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Afsdarif\\TranslationManager\\TranslationManagerProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
