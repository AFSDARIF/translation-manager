{
  "name": "musa11971/filament-translation-manager",
  "description": "TBA",
  "keywords": [
    "laravel",
    "localization",
    "filament",
    "translation"
  ],
  "homepage": "https://github.com/musa11971/filament-translation-manager",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "musa11971\\FilamentTranslationManager\\": "src/"
    }
  },
  "authors": [
    {
      "name": "musa11971",
      "role": "Maintainer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "filament/filament": "^2.17",
    "filament/notifications": "^2.17",
    "spatie/laravel-translation-loader": "^2.7",
    "spatie/laravel-package-tools": "^1.10"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "musa11971\\FilamentTranslationManager\\FilamentTranslationManagerProvider"
      ]
    }
  },
  "prefer-stable": true,
  "require-dev": {
    "tightenco/duster": "^1.1"
  }
}
